				{
					"name": "Tentando editar um produto inexistente",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"\r",
									"let validIds = [1, 2, 3, 4, 5];\r",
									"\r",
									"\r",
									"function gerarIdString(tamanho) {\r",
									"    let chars = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';\r",
									"    let resultado = '';\r",
									"    for (let i = 0; i < tamanho; i++) {\r",
									"        resultado += chars.charAt(Math.floor(Math.random() * chars.length));\r",
									"    }\r",
									"    return resultado;\r",
									"}\r",
									"\r",
									"// Gerar ID inválido string\r",
									"let invalidStrId = gerarIdString(15);\r",
									"\r",
									"// Gerar ID inválido numérico\r",
									"let invalidNumId = Math.floor(Math.random() * 1000) + 1000;\r",
									"\r",
									"let escolha = Math.random();\r",
									"let produtoId;\r",
									"\r",
									"if (escolha < 0.33) {\r",
									"    produtoId = validIds[Math.floor(Math.random() * validIds.length)]; \r",
									"} else if (escolha < 0.66) {\r",
									"    produtoId = invalidNumId; \r",
									"} else {\r",
									"    produtoId = invalidStrId; \r",
									"}\r",
									"\r",
									"pm.variables.set(\"produtoId\", produtoId);\r",
									"console.log(\"ID usado no teste:\", produtoId);\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"let idUsado = pm.variables.get(\"produtoId\");\r",
									"let status = pm.response.code;\r",
									"\r",
									"if(!isNaN(idUsado) && idUsado <= 5){\r",
									"    pm.test(\"Deve atualizar com sucesso (200)\", function () {\r",
									"        pm.expect(status).to.eql(200);\r",
									"    });\r",
									"} else {\r",
									"    pm.test(\"Deve retornar erro para ID inválido (404)\", function () {\r",
									"        pm.expect(status).to.eql(404);\r",
									"    });\r",
									"}\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
